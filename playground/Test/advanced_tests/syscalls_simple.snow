module: SyscallsSimple
    import: std_io
    import: syscall.time
    
    function: main
        returns: void
        body:
            // 测试时间相关的系统调用
            declare startTime: long = time.mono_ms()
            std_io.println("Start time: " + startTime)
            
            // 测试syscall函数
            declare result: long = syscall("0x1703")  // 获取单调时钟的毫秒数
            std_io.println("Syscall result: " + result)
        end body
    end function
end module