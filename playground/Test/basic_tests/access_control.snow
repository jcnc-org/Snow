module: AccessControl
    import: std_io
    
    globals:
        declare _secret: int = 42      // 私有全局变量
        declare visible: int = 1       // 公有全局变量
    
    function: _hidden                  // 私有函数
        returns: int
        body:
            return 100
        end body
    end function
    
    function: publicFunc
        returns: int
        body:
            return _secret + 1         // 在同一模块内可以访问私有成员
        end body
    end function
    
    function: main
        returns: void
        body:
            std_io.println("Public variable: " + visible)
            std_io.println("Public function result: " + publicFunc())
            // 注意：无法从这里访问_secret或_hidden()，因为它们是私有的
        end body
    end function
end module