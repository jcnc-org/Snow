module: app
    import: os

    function: main
        params:
            declare args: any
        body:
            declare ret: int

            // 向标准错误输出写一条消息
            ret = os.stderrWrite("这是错误信息！")
            ret = os.stderrWrite("\n")

            // 打印返回值，正常情况下是 0
            os.println("stderrWrite 返回值: " + ret)
        end body
    end function
end module
