module: Main
    import: ModuleA,os
    function: main
        returns: void
        body:
            declare a: int = ModuleA.sum(1,2)
        end body
    end function
end module

module: ModuleA
    globals:
        declare a: int = 10
    function: sum
        params:
            declare a: int
            declare b: int
        returns: int
        body:
            return a + b
        end body
    end function
end module