module: Main
    import: ModuleA,os
    function: main
        returns: void
        body:
            declare a: byte = ModuleA.sum(1b,2b)
        end body
    end function
end module

module: ModuleA
    globals:
        declare a: byte = 10b
    function: sum
        params:
            declare a: byte
            declare b: byte
        returns: byte
        body:
            return a + b
        end body
    end function
end module