module: Main
    import: syscall.fs
    import: syscall.fd
    import: std.stdio
    import: ModuleA
    globals:
        declare c: int = 10
    function: main
        returns: void
        body:
             c = ModuleA.getA()
             stdio.print(c)
        end body
    end function
end module

module: ModuleA
    globals:
        declare a: int = 2
    function: getA
        returns: int
        body:
            return a
        end body
    end function
end module
