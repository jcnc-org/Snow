module: Student
import : Person

struct : Student extends Person
    fields:
        declare id: int

    init:
        params:
            declare name: int
        body:
            super(name)
        end body
    end init

    // 重写父类方法，演示多态
    function: getName
        returns: int
        body:
            return super.getName() + 1000
        end body
    end function
end struct
end module
