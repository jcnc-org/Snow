module: app
    import: os

    function: main
        params:
            declare args: any
        body:
            declare fd: int
            // 打开 .env 文件（只读，flags=0）
            fd = os.open(".env", 0)
            // 读取100000
            declare content: any
            content = os.read(fd, 100000)
            // 打印文件内容（带自动换行）
            os.println(content)
            // 关闭文件
            os.close(fd)
        end body
    end function
end module
