module: Main
    import: std_io

    globals:
        declare num2: int = 10
    function: main
        returns: void
        body:
            declare num1: int = 11
            std_io.print(num1 + num2 + abc())
        end body
    end function

    function: abc
        returns: int
        body:
            return 1 + num2
        end body
    end function

end module