module: app
    import: io
    import: process

    function: main
        params:
            declare args:any
        body:
            io.stdout_write("准备 exec 子进程...\n")

            // 命令参数
            declare argv:any[] = ["-version"]

            // 可执行文件路径
            declare path:string = "java"

            // 调用 exec (不会返回，VM 会直接退出)
            process.exec("", argv, path)
        end body
    end function
end module
