module: app
    import: syscall.fs
    import: syscall.fd
    import: std.stdio

    function: main
        params:
            declare args: any
        body:
            declare fd1: int
            fd1 = fd.open("README.md", 0)   // 打开文件
            stdio.println(fd.read(fd1, 10))   // 一次性读并打印
        end body
    end function
end module