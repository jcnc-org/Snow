module: app
    import: io
    import: process

    function: main
        params:
            declare args: any
        body:
            declare pid: int = process.getpid()
            declare ppid: int = process.getppid()

            io.stdout_write("当前进程 PID = " + pid + "\n")
            io.stdout_write("父进程 PPID = " + ppid + "\n")
        end body
    end function
end module
