module: app
    import: os

    function: main
        params:
            declare args: any
        body:
            declare fd: int
            fd = os.syscall("0x1000", "README.md", 0)   // 打开文件
            os.println(os.read(fd, 100000))   // 一次性读并打印
        end body
    end function
end module
